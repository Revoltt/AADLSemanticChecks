PACKAGE P1
PUBLIC
	SYSTEM S1
		PROTOTYPES
			U : DATA;
		FEATURES
			V : IN EVENT PORT;
		FLOWS
			W : FLOW SINK V;
	END S1;
		
	SYSTEM S2 EXTENDS S1
	END S2;
		
	SYSTEM S3 EXTENDS S2
		PROTOTYPES
			U1 : REFINED TO DATA;
		FEATURES
			V1 : REFINED TO IN EVENT PORT;
		FLOWS
			W1 : REFINED TO FLOW SINK;
	END S3;
END P1;