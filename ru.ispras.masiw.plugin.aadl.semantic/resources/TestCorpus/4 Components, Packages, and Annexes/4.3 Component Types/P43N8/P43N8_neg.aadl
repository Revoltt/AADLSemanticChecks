PACKAGE P1
PUBLIC
	ABSTRACT A 
	END A;
	
	DATA D 
	END D;
	
	SYSTEM S1
		PROTOTYPES
			X : ABSTRACT A;
	END S1;
	
	SYSTEM S2
	END S2;
	
	SYSTEM S3 EXTENDS S2 ( X => DATA D )
	END S3;
	
END P1;

PACKAGE P2
PUBLIC
	WITH P1;
	S RENAMES SYSTEM P1::S2;
	
	DATA D 
	END D;
	
	SYSTEM S3 EXTENDS S ( X => DATA D )
	END S3;
END P2;